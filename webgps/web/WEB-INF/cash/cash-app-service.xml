<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans.dtd">

<beans>
	<description>app-cash-service-bean</description>
	<!-- Add Managers here -->	
	<bean id="cashService"
		class="com.sosgps.v21.cash.service.impl.CashServiceImpl">
		<property name="cashDao">
			<ref bean="CashDao" />
		</property>		
		<property name="targetDao">
			<ref bean="TargetDao" />
		</property>
		<property name="targetTemplateDao">
			<ref bean="TargetTemplateDao" />
		</property>
	</bean>
</beans>
